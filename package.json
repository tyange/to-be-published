    "axios": "^1.6.5",
    "pinia": "^2.1.7",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
